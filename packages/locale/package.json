{
  "name": "@yh-ui/locale",
  "version": "0.1.1",
  "description": "YH-UI internationalization locale files",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./lang/*": {
      "types": "./dist/lang/*.d.ts",
      "import": "./dist/lang/*.mjs",
      "require": "./dist/lang/*.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/1079161148/yh-ui.git",
    "directory": "packages/locale"
  },
  "keywords": [
    "yh-ui",
    "locale",
    "i18n"
  ],
  "homepage": "https://yh-ui.dev",
  "author": "YH-UI Team",
  "license": "MIT"
}
