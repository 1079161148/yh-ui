{
  "name": "@yh-ui/components",
  "version": "0.0.1",
  "description": "YH-UI Vue 3 Components",
  "type": "module",
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    "**/*.sass"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "import": "./dist/resolver.mjs"
    },
    "./*": {
      "import": "./dist/*/*.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch"
  },
  "dependencies": {
    "@yh-ui/hooks": "workspace:*",
    "@yh-ui/theme": "workspace:*",
    "@yh-ui/utils": "workspace:*",
    "@yh-ui/locale": "workspace:*",
    "dayjs": "catalog:",
    "viewerjs": "^1.11.7"
  },
  "peerDependencies": {
    "vue": "catalog:"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "vite": "catalog:",
    "vite-plugin-dts": "^4.4.0",
    "vue": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/1079161148/yh-ui.git",
    "directory": "packages/components"
  },
  "keywords": [
    "yh-ui",
    "vue3",
    "components"
  ],
  "homepage": "https://yh-ui.dev",
  "author": "YH-UI Team",
  "license": "MIT"
}
