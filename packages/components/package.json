{
  "name": "@yh-ui/components",
  "version": "0.1.10",
  "description": "YH-UI Vue 3 Components",
  "type": "module",
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    "**/*.sass",
    "dist/**/index.mjs"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "import": "./dist/resolver.mjs",
      "require": "./dist/resolver.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@yh-ui/hooks": "workspace:*",
    "@yh-ui/locale": "workspace:*",
    "@yh-ui/theme": "workspace:*",
    "@yh-ui/utils": "workspace:*",
    "async-validator": "^4.2.5",
    "dayjs": "catalog:",
    "highlight.js": "^11.11.1",
    "markdown-it": "^14.1.1",
    "viewerjs": "^1.11.7"
  },
  "peerDependencies": {
    "vue": "catalog:"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@vitejs/plugin-vue": "^6.0.0",
    "sass": "^1.83.1",
    "typescript": "catalog:",
    "unbuild": "^3.3.1",
    "vue": "catalog:",
    "vue-sfc-transformer": "^0.1.17",
    "vue-tsc": "^2.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/1079161148/yh-ui.git",
    "directory": "packages/components"
  },
  "keywords": [
    "yh-ui",
    "vue3",
    "components"
  ],
  "homepage": "https://1079161148.github.io/yh-ui/",
  "author": "YH-UI Team",
  "license": "MIT"
}
